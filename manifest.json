{

  "manifest_version": 2,
  "name": "SaveMyTabs",
  "icons": {
    "64": "icons/icon.png"
  },
  "version": "1.0",

  "description": "Allows you to keep tabs after restarting",

  "permissions": [
    "tabs",
    "storage"
  ],

  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "SaveMyTabs",
    "default_popup": "popups/enabledisable.html"
  },

  "background": {
    "scripts": ["background_scripts/opentabs.js"]
  }
}
